# Non-linear (diffeomorphic) registration options
mdt_contrast=fa
mdt_creation_strategy=iterative
mdt_iterations=6
diffeo_transform_parameters=0.5,3,0.5

# Various optional parameters

custom_predictor_string=nos2_vs_cvn
