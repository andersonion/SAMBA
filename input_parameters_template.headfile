# Label options
create_labels=1
label_atlas_name=chass_symmetric2
label_space=pre_rigid,post_affine # pre_rigid,post_rigid(pre_affine),post_affine,MDT, and/or atlas	
  
# Masking/skull-stripping options	
skull_strip_contrast=dwi
threshold_code=4
do_mask=1
port_atlas_mask=0
#pre_masked=0

# VBA Options
vba_analysis_software=surfstat
